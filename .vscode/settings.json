{
    "rust-analyzer.rustfmt.extraArgs": [
        "+nightly"
    ],
    "rust-analyzer.cargo.targetDir": "./target/rust-analyzer",
    "rust-analyzer.check.noDefaultFeatures": true,
    "rust-analyzer.check.workspace": false,
    "editor.rulers": [
        100
    ],
    "cSpell.words": [
        "apimachinery",
        "Autoscaler",
        "banzaicloud",
        "BYOC",
        "Citext",
        "DTO",
        "INET",
        "intstr",
        "jsonwebtoken",
        "kubeconfig",
        "leptos",
        "middlewares",
        "onpatr",
        "openapi",
        "opentelemetry",
        "otlp",
        "padawan",
        "PKCE",
        "preprocess",
        "proxied",
        "reauthenticated",
        "reauthentication",
        "regcred",
        "rustfmt",
        "sqlx",
        "Stylesheet",
        "TIMESTAMPTZ",
        "tungstenite",
        "wayy",
        "Workspaced"
    ],
    "nixEnvSelector.nixFile": "${workspaceFolder}/shell.nix",
    "rust-analyzer.showUnlinkedFileNotification": false,
    "editor.formatOnSave": true,
}